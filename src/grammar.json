{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "pyjsx",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "python_code"
          },
          {
            "type": "SYMBOL",
            "name": "jsx_element"
          },
          {
            "type": "SYMBOL",
            "name": "jsx_fragment"
          },
          {
            "type": "SYMBOL",
            "name": "jsx_expression"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          }
        ]
      }
    },
    "python_code": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^<{]+"
            },
            {
              "type": "PATTERN",
              "value": "[<{]"
            }
          ]
        }
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[^<>{]+"
        }
      }
    },
    "jsx_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "<"
            },
            {
              "type": "SYMBOL",
              "name": "element_name"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "jsx_attribute"
              }
            },
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "jsx_element"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "jsx_fragment"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "jsx_expression"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "content"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "</"
            },
            {
              "type": "SYMBOL",
              "name": "element_name"
            },
            {
              "type": "STRING",
              "value": ">"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "<"
            },
            {
              "type": "SYMBOL",
              "name": "element_name"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "jsx_attribute"
              }
            },
            {
              "type": "STRING",
              "value": "/>"
            }
          ]
        }
      ]
    },
    "jsx_fragment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<>"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "jsx_element"
              },
              {
                "type": "SYMBOL",
                "name": "jsx_fragment"
              },
              {
                "type": "SYMBOL",
                "name": "jsx_expression"
              },
              {
                "type": "SYMBOL",
                "name": "content"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "</>"
        }
      ]
    },
    "jsx_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "python_expression"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "jsx_attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute_name"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "attribute_string_value"
                },
                {
                  "type": "SYMBOL",
                  "name": "jsx_expression"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "jsx_spread_attribute"
        }
      ]
    },
    "jsx_spread_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "..."
        },
        {
          "type": "SYMBOL",
          "name": "python_expression"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "python_expression": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^}]+"
            },
            {
              "type": "PATTERN",
              "value": "}"
            }
          ]
        }
      }
    },
    "element_name": {
      "type": "PATTERN",
      "value": "[A-Za-z_]\\w*(?:\\.[A-Za-z_]\\w*)*"
    },
    "attribute_name": {
      "type": "PATTERN",
      "value": "[^\\s='\\\"<>{}]+"
    },
    "attribute_string_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
